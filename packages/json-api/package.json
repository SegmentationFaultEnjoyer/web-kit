{
  "name": "@dl/json-api",
  "description": "API Caller for JSON API",
  "version": "1.0.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "browser": "./lib/json-api.min.js",
  "unpkg": "./lib/json-api.min.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias -p tsconfig.json"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4"
  }
}
