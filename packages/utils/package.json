{
  "name": "@dl/utils",
  "version": "1.0.0",
  "description": "Typescript-base utils for Distributed Lab projects",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "browser": "./lib/json-api.min.js",
  "unpkg": "./lib/json-api.min.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "DL | Utils"
  },
  "sideEffects": false,
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias -p tsconfig.json",
    "clean": "rm -rf lib"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4"
  }
}
